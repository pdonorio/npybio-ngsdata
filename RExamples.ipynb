    "# Using the library\n",
    "library(\"RSQLite\")\n",
    "sqlite <- dbDriver(\"SQLite\")\n",
    "fdb <- \"fsql/mydb.db\"\n",
    "db <- dbConnect(sqlite, fdb)"
   ]
  },
